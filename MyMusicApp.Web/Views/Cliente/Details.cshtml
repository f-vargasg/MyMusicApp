@model MyMusicApp.Web.ViewModel.SucursalProductoVM

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Detalles de la Sucursal</h1>

@if (Model.Error == null)
{
<div>
    <h4>@Html.DisplayNameFor(model => model.Sucursal.DirSucursal)</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Sucursal.HorarioSucursal)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Sucursal.HorarioSucursal)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Sucursal.TelefonoSucursal)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Sucursal.TelefonoSucursal)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NombreTipo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NombreTipo)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { /* id = Model.PrimaryKey */ }) |
    <a asp-action="Index">Back to List</a>
</div>
}
else
{
    <h4>Ha ocurrido un error</h4>
    <div>@Html.DisplayFor(model => model.Error.MensajeError)</div>
}